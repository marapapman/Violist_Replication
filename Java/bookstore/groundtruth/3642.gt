select e.article_id as e_article_id, e.article_title as e_article_title, e.editorial_cat_id as e_editorial_cat_id, e.item_id as e_item_id, e1.editorial_cat_id as e1_editorial_cat_id, e1.editorial_cat_name as e1_editorial_cat_name, i.item_id as i_item_id, i.name as i_name  from editorials e, editorial_categories e1, items i where e1.editorial_cat_id=e.editorial_cat_id and i.item_id=e.item_id   order by e.article_title Asc
